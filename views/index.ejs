  <meta name="twitter:description" content="<%= __('twitter_description') %>">
  <meta name="twitter:image:src" content="https://dotsave.app/assets/img/snapthumb.jpg">
  <meta name="twitter:site" content="dotsave.app">
  <!--
  Thẻ quy định nội dung hiển thị khi preview trên mạng xã hội Facebook
-->
